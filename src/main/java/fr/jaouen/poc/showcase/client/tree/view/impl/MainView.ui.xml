<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			text-align: center;
			position:relative;
		}
	</ui:style>
	<g:HTMLPanel>
		<div class="panel panel-primary col-sm-12">
			<div class="alert alert-info center">
				<g:ListBox ui:field="listBoxTest"/>
				<g:TextBox ui:field="txtBoxTest"/>
				<g:Button styleName="btn btn-success" ui:field="btnReturn" text="Sauvegarder"/>
				<g:Label ui:field="lblUser"/>
			</div>
			<div class="col-sm-5">
				<c:CellTree ui:field="leftTree"/>
			</div>	
			<div class="col-sm-2">
				<g:Button styleName="btn btn-success" ui:field="btnPeuple" text=">"/>
			</div>	
			<div class="col-sm-5">
				<c:CellTree ui:field="rightTree"/>
			</div>			
		</div>	
	</g:HTMLPanel>
</ui:UiBinder> 